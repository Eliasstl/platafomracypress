        >
          <div class="form-group">
            <label for="nomeagendamento">Nome do Agendamento</label>
            <input
              type="text"
              class="form-control"
              id="nomeagendamento"
              name="nomeagendamento"
              placeholder="Nome do agendamento"
            />
          </div>

          <div class="row">
            <div class="col-md-2">
              <label for="tempo">Tempo</label>
              <select id="tempo" class="form-control" name="tempo">
                <option value="30">30 minutos</option>
                <option value="60">1 hora</option>
                <option value="120">2 horas</option>
                <option value="180">3 horas</option>
              </select>
            </div>
          </div>

          <div class="row">
            <div class="col-md-2">
              <label for="inputData">Data para terminar :</label>
              <input
                type="text"
                class="form-control"
                id="inputData"
                name="data"
                placeholder="dd/mm/yyyy"
              />
              <small class="form-text text-danger" id="errorData"></small>
              <p class="mt-2">Se desejar escolha uma data para terminar</p>
            </div>
          </div>

