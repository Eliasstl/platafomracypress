<script>
  // obter os elementos HTML relevantes
  const dropdownButton = document.querySelector("#dropdownMenuButton");
  const dropdownMenu = document.querySelector(".dropdown-menu");

  // adicionar um evento de clique para as opções
  dropdownMenu.addEventListener("click", (event) => {
    event.preventDefault();
    const option = event.target;

    // atualizar o valor do botão com a escolha da pessoa
    const optionText = option.textContent;
    dropdownButton.textContent = `Repetir a cada ${optionText}`;

    // atualizar o valor do menu suspenso com a escolha da pessoa
    const optionMinutos = option.dataset.minutos;
    dropdownButton.dataset.minutos = optionMinutos;
  });

  // Função para formatar a data no padrão dd/mm/yyyy
  function formatarData(data) {
    if (!data) {
      return "";
    }
    var dia = data.getDate().toString().padStart(2, "0");
    var mes = (data.getMonth() + 1).toString().padStart(2, "0");
    var ano = data.getFullYear();
    return dia + "/" + mes + "/" + ano;
  }

  $(document).ready(function () {
    // Seleciona o input de data
    var inputData = $("#inputData");

    // Define a data mínima como a data atual
    var dataMinima = new Date();
    inputData.attr("min", formatarData(dataMinima));

    // Adiciona um listener de mudança de valor no input de data
    inputData.on("input", function () {
      // Obtém a data atual
      var dataAtual = new Date();

      // Obtém a data digitada pelo usuário
      var dataDigitada = inputData.val().replace(/\D/g, "");
      var dia = dataDigitada.substr(0, 2);
      var mes = dataDigitada.substr(2, 2);
      var ano = dataDigitada.substr(4, 4);

      // Verifica se a data digitada é válida e não é menor do que a data atual
      if (!/^\d{2}\/\d{2}\/\d{4}$/.test(inputData.val())) {
        // Mostra uma mensagem de erro
        $("#errorData").text("A data deve estar no formato dd/mm/yyyy.");
        inputData.addClass("is-invalid");
      } else {
        dataDigitada = new Date(ano, mes - 1, dia);
        if (isNaN(dataDigitada) || dataDigitada < dataAtual) {
          // Mostra uma mensagem de erro
          $("#errorData").text("A data deve ser maior ou igual à data atual.");
          inputData.addClass("is-invalid");
        } else {
          // Remove a mensagem de erro
          $("#errorData").text("");
          inputData.removeClass("is-invalid");

          // Formata a data digitada no padrão dd/mm/yyyy
          inputData.val(formatarData(dataDigitada));
        }
      }
    });
  });
</script>
